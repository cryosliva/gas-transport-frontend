{
  "name": "tram-web",
  "version": "1.0.0",
  "description": "Web application for Tram",
  "main": "server/server.js",
  "scripts": {
    "start": "cd server && node server.js",
    "server": "cd server && nodemon server.js",
    "client": "./node_modules/.bin/webpack-dev-server --config ./client/webpack.dev.config.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "./node_modules/.bin/webpack --config ./client/webpack.dev.config.js --mode none",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cryosliva/gas-transport-frontend.git"
  },
  "author": "Karolina Tretyakova",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cryosliva/gas-transport-frontend/issues"
  },
  "homepage": "https://github.com/cryosliva/gas-transport-frontend#readme",
  "dependencies": {
    "classnames": "^2.2.6",
    "concurrently": "^3.5.1",
    "express": "^4.16.3",
    "leaflet": "^1.4.0",
    "path": "^0.12.7",
    "ramda": "^0.26.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-inlinesvg": "^0.8.3",
    "react-leaflet": "^2.1.4",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.28.2",
    "redux": "^4.0.0",
    "redux-observable": "^1.0.0",
    "redux-thunk": "^2.3.0",
    "typed-actions": "^0.9.1",
    "webpack": "^4.12.0",
    "webpack-dev-server": "^3.1.14"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "file-loader": "^3.0.1",
    "image-webpack-loader": "^4.6.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "nodemon": "^1.17.5",
    "style-loader": "^0.21.0",
    "svg-inline-loader": "^0.8.0",
    "webpack-cli": "^3.0.3"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-flow-strip-types"
    ]
  }
}
